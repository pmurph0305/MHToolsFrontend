  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "fetch-mock": "^7.3.3",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "prettier": "1.17.1",
    "pretty-quick": "^1.10.0",
    "redux-mock-store": "^1.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"

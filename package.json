{
  "name": "mhtoolsfront",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://pmurph0305.github.io/MHToolsFrontend/",
  "dependencies": {
    "node-sass": "^4.12.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1",
    "recharts": "^1.5.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "sass": "node-sass -w sass -o public/css",
    "start": "react-scripts start",
    "build": "react-scripts build",
